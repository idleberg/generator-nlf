    "update-notifier": "^2.5.0",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.16.0",
